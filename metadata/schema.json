{
  "file_types": {
    "text": [".csv", ".html", ".htm", ".json", ".log", ".txt", ".xml"],
    "binary": [
      ".bmp",
      ".bin",
      ".dd",
      ".dat",
      ".dmp",
      ".gif",
      ".iso",
      ".jpeg",
      ".jpg",
      ".mem",
      ".png",
      ".raw",
      ".tif",
      ".tiff"
    ],
    "script": [".bat", ".js", ".pl", ".py", ".sh"],
    "database": [".db", ".sqlite"],
    "config": [".cfg", ".ini"]
  },
  "log_patterns": {
    "android": "(\\d{2}-\\d{2}) (\\d{2}:\\d{2}:\\d{2}\\.\\d{3})\\s+(\\d+)\\s+(\\d+)\\s+([DEWIFV])\\s+([\\w\\.]+):\\s+(.*)",
    "apache": "\\[(\\w{3} \\w{3} \\d{2} \\d{2}:\\d{2}:\\d{2} \\d{4})\\]\\s+\\[(\\w+)\\]\\s+(.*)",
    "hadoop": "(\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2},\\d{3}) (\\w+) \\[.*?\\] ([\\w.]+): (.*)",
    "hdfs": "(\\d{6} \\d{6} \\d{3}) (\\w+) (\\S+): (.*)",
    "hpc": "(\\d+)\\s+(\\S+)\\s+(\\S+)\\s+(\\S+)\\s+(\\d+)\\s+(\\d+)\\s+(.*)",
    "linux": "(\\w{3} \\d{2} \\d{2}:\\d{2}:\\d{2}) (\\w+)\\s+(\\S+)\\((\\S+)\\)\\[(\\d+)\\]: (.*)",
    "mac": "(\\w{3} \\s+\\d{1,2} \\d{2}:\\d{2}:\\d{2}) (\\S+)\\s+(\\S+)\\[.*?\\]:\\s+(.*)",
    "openssh": "(\\w{3} \\d{2} \\d{2}:\\d{2}:\\d{2}) (\\w+)\\s+sshd\\[\\d+\\]: (.*)",
    "spark": "(\\d{2}/\\d{2}/\\d{2} \\d{2}:\\d{2}:\\d{2}) (\\w+) ([\\w.]+): (.*)",
    "windows": "(\\d{4}-\\d{2}-\\d{2}) (\\d{2}:\\d{2}:\\d{2}), (\\w+)\\s+(\\w+)\\s+(.*)"
  }
}
